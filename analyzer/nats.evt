import NATS;
import Zeek_NATS;

protocol analyzer NATS over TCP:
    parse originator with NATS::ClientMessages,
    parse responder with NATS::ServerMessages;

on NATS::ClientData -> event NATS::request($conn, $is_orig, self.payload);
on NATS::ServerData -> event NATS::reply($conn, $is_orig, self.payload);
